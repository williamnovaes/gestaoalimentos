--

CREATE TABLE rev_usuario (
    id integer NOT NULL,
    _revisao bigint NOT NULL,
    tipo_alteracao smallint,
    cpf character varying(11),
    data_nascimento date,
    email character varying(100),
    login character varying(50),
    nome character varying(150),
    rg character varying(20),
    senha character varying(150),
    situacao character varying(10) DEFAULT 'ATIVO'::character varying,
    _empresa integer,
    _nivel integer
);


ALTER TABLE rev_usuario OWNER TO postgres;

